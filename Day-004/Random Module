# random.random() - float between 0.0 inclusive and 1.0 exclusive
randNum0to1 = random.random()
print(randNum0to1)

# random.uniform() - float inclusive to a and b
randFloat = random.uniform(1, 10)
print(randFloat)

# create coin flip program
# prints "Heads" or "Tails" - depends on random number generated

# creating headsOrTails variable and setting it to choose randomly between 1 and 2
# heads = 1; tails = 2
headsOrTails = random.randint(1,2)
heads = 1
tails = 2

# determining what prints depending on the random int headsOrTails
if headsOrTails == 1:
    print("Heads")
else:
    print("Tails")

